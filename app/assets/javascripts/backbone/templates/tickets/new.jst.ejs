<h1>New ticket</h1>

<form id="new-ticket" name="ticket">
  <div class="form-group">
    <label for="title"> Title:</label>
    <input class="form-control" type="text" name="title" id="title" value="<%= title %>" >
  </div>

  <div class="form-group">
    <label for="description"> Description:</label>
    <input  class="form-control" type="text" name="description" id="description" value="<%= description %>" >
  </div>

  <div class="actions">
    <input class="btn btn-default" type="submit" value="Create Ticket" />
  </div>

</form>

<a href="#/index">Back</a>